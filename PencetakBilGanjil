import java.util.*;
public class PencetakBilGanjil{
  public static void main(String[]argv){
    Scanner sc = new Scanner(System.in);
    System.out.print("Jumlah Bilangan Ganjil= ");
    int n = sc.nextInt();
    int i=1;
    int bilGanjil=1;
    while(i<=n){
      System.out.println(bilGanjil);
      bilGanjil +=2;
      i++;
    }        
  }
}
